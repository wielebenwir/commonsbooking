{
  "name": "wielebenwir/commonsbooking",
  "description": "~",
  "version": "0.0.1",
  "config": {
    "platform-check": false,
    "allow-plugins": {
      "composer/installers": true,
      "php-http/discovery": true
    }
    },
  "autoload": {
    "files": [
      "vendor/autoload.php",
      "includes/OptionsArray.php",
      "includes/Shortcodes.php",
      "includes/Template.php",
      "includes/TemplateParser.php",
      "includes/Users.php"
    ],
    "psr-4": {"CommonsBooking\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {
      "CommonsBooking\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/mustardbees/cmb-field-select2"
    }
  ],
  "require": {
    "php": ">=7.3",
    "cmb2/cmb2": "^2.7",
    "mustardbees/cmb-field-select2": "^3.0",
    "geocoder-php/nominatim-provider": "^5.2",
    "php-http/curl-client": "^2.1",
    "nyholm/psr7": "^1.3",
    "opis/json-schema": "^1.0",
    "scssphp/scssphp": "^1.10.0",
    "ext-json": "*",
    "symfony/cache": "^5.4",
    "symfony/service-contracts": "v1.1.11",
    "eluceo/ical": "^2.5",
    "phpmailer/phpmailer": "^6.6"
  },
  "require-dev": {
    "mockery/mockery": "1.*",
    "phpunit/phpunit": "^9.6",
    "phpunit/php-code-coverage": "^9.2",
    "yoast/phpunit-polyfills": "^1.0",
    "wp-coding-standards/wpcs": "^2.3"
  },

  "scripts" : {
    "phpunit": "./vendor/bin/phpunit",
    "test": "@phpunit -c phpunit.xml.dist"
  }
}
